# enable color support of ls and also add handy aliases
if [ -x /usr/bin/dircolors ]; then     test -r ~/.dircolors && eval "$(dircolors -b ~/.dircolors)" || eval "$(dircolors -b)";     alias ls='ls --color=auto'     alias grep='grep --color=auto';     alias fgrep='fgrep --color=auto';     alias egrep='egrep --color=auto'; fi
# some more ls aliases
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'
alias l.='ls -lAd .* | grep -v /'
alias cls='clear'
alias lz='ls -1 --color=auto'
alias lsp='ls -d1 $PWD/*'
# Add an "alert" alias for long running commands.  Use like so:
#   sleep 10; alert
alias alert='notify-send --urgency=low -i "$([ $? = 0 ] && echo terminal || echo error)" "$(history|tail -n1|sed -e '\''s/^\s*[0-9]\+\s*//;s/[;&|]\s*alert$//'\'')"'
notify-send --urgency=low -i "$([ $? = 0 ] && echo terminal || echo error)"
notify-send --urgency=low -i "$([ $? = 0 ] && echo terminal || echo error)" "summary"
notify-send --urgency=low -i "$([ $? = 0 ] &) "ry"
notify-send --urgency=low -i "$([ $? = 0 ] && echo terminal) "ry"
notify-send --urgency=low -i "$([ $? = 0 ] && echo terminal)" "ry"
